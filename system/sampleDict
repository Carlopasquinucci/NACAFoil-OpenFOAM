FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;

interpolationScheme cellPoint;

sets
(
    point
    {
        type        cloud;
        axis        x;
        points      ((1.25 0 0));
    }
);

fields (k omega epsilon);

